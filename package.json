{
  "name": "typed-emitters",
  "version": "6.0.0",
  "scripts": {
    "test": "jest",
    "typecheck": "npx tsc",
    "lint": "eslint --fix ./src",
    "prettier": "prettier --write ./src",
    "bundle": "tsup src/lib --format esm,cjs,iife --legacy-output --dts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier",
      "npm run lint"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.1",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "author": "<d.vifer@gmail.com>",
  "license": "MIT",
  "description": "Simple and convenient event emitters with separate interfaces for consumers.",
  "keywords": [
    "event-emitter",
    "events",
    "emitter",
    "typescript",
    "typed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/denvifer/typed-emitters"
  }
}